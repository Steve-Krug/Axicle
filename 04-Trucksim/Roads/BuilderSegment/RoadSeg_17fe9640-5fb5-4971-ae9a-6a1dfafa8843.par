! Segment 4, clothoid
IPATHSEG 4
SEGMENT_TYPE 4
SEGMENT_RHO_DS_ENTERING 0.001271
SEGMENT_RHO_DS_DEPARTING 0.000000
SEGMENT_LENGTH 14.000000

! Segment 5, clothoid
IPATHSEG 5
SEGMENT_TYPE 4
SEGMENT_RHO_DS_ENTERING 0.000000
SEGMENT_RHO_DS_DEPARTING -0.002674
SEGMENT_LENGTH 26.000000

! Segment 6, radius
IPATHSEG 6
SEGMENT_TYPE 3
SEGMENT_RADIUS -374.000000
SEGMENT_ARC 0.781306

! Segment 7, clothoid
IPATHSEG 7
SEGMENT_TYPE 4
SEGMENT_RHO_DS_ENTERING -0.002674
SEGMENT_RHO_DS_DEPARTING 0.000000
SEGMENT_LENGTH 39.000000

! Segment 8, straight
